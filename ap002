#include <iostream>
using namespace std;
int main() {
    int n = 0;
    int c = 1;
    string line;
    getline(cin, line);
    int sum = 0;
    int a = line.length();
    for (int i = 0; i < a; i++) {
        if (isdigit(line[i])) {
            n *=  c ;
            n +=  line[i];
            c *=  10;
        }
        if (!isdigit(line[i])) {
                switch (line[i]) {
                    case '+':
                        sum = sum + n;
                        break;
                    case '-':
                        sum = sum - n;
                        break;
                    case '*':
                        sum = sum * n;
                        break;
                    case '/':
                        sum = sum / n;
                        break;
                    default:
                        cout << "error";
                }
                n = 0;
                c = 1;
        }
        }
    cout << sum;
        return 0;
    }
